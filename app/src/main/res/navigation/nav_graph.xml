<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/storeItemsFragment">
    <fragment
        android:id="@+id/storeItemsFragment"
        android:name="araikovich.inc.sales.luxhome.ui.items.StoreItemsFragment"
        android:label="StoreItemsFragment" >
        <action
            android:id="@+id/action_storeItemsFragment_to_storeItemDetailFragment2"
